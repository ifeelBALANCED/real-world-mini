{
  "name": "nestjs-admin-front",
  "version": "0.1.0",
  "author": {
    "name": "MRX",
    "url": "https://github.com/mmnkuh"
  },
  "engines": {
    "node": "14.20.0",
    "yarn": "1.22.19"
  },
  "scripts": {
    "build": "npm run typecheck && vite build",
    "commit": "git-cz",
    "dev": "vite",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false yarn --prefix react-world-mini && yarn run build --prefix react-world-mini",
    "format": "prettier --write \"./**/*.{ts,tsx}\" && git update-index --again ",
    "lint": "eslint \"./src/**/*.{ts,tsx}\" --fix",
    "prepare": "husky install",
    "serve": "yarn run build && vite preview --port 3000",
    "test:e2e": "start-server-and-test serve http://127.0.0.1:3000/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test serve http://127.0.0.1:3000/ 'cypress run'",
    "test:unit": "jest --watch",
    "test:unit:ci": "jest",
    "test:unit:coverage": "jest --silent --watchAll=false --coverage",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{css,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "eslint",
      "prettier --write"
    ],
    "package.json": "npx sort-package-json"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@hookform/resolvers": "^2.8.8",
    "@reduxjs/toolkit": "^1.8.1",
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.3.1",
    "@types/js-cookie": "^3.0.1",
    "@types/lodash": "^4.14.181",
    "@types/react-redux": "^7.1.23",
    "@types/react-router": "^5.1.18",
    "@types/redux": "^3.6.0",
    "@types/redux-persist": "^4.3.1",
    "@types/yup": "^0.29.13",
    "antd": "^4.19.5",
    "axios": "^0.26.1",
    "classnames": "^2.3.1",
    "js-cookie": "^3.0.1",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lodash": "^4.17.21",
    "node-sass": "^4.14.1",
    "path-to-regexp": "^6.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.29.0",
    "react-redux": "^7.2.8",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "redux": "^4.1.2",
    "redux-persist": "^6.0.0",
    "sass": "^1.49.11",
    "sweetalert2": "^9.17.1",
    "sweetalert2-react-content": "^5.0.0",
    "vite-plugin-imp": "^2.1.7",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@ant-design/pro-layout": "^6.16.0",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@commitlint/cz-commitlint": "^16.2.3",
    "@cypress/vite-dev-server": "^2.2.2",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.22",
    "@types/react": "^17.0.41",
    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vitejs/plugin-legacy": "^1.7.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.4.4",
    "commitizen": "^4.2.4",
    "cypress": "^9.5.2",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.2",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-testing-library": "^5.1.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "postcss": "^8.4.12",
    "postcss-import": "^14.0.2",
    "prettier": "^2.6.0",
    "sort-package-json": "^1.55.0",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2",
    "vite": "2.5.4"
  }
}
